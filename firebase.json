{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "indexes.json"
  },
  "hosting": {
    "site": "trinkey",
    "public": "public",
    "ignore": [
      "firebase.json",
      "deploy.bat",
      "**/.*",
      "**/node_modules/**",
      "public/template.html",
      ".gitignore"
    ],
    "rewrites": [
      {
        "source": "/changelog",
        "destination": "/patch.html"
      },
      {
        "source": "/photography",
        "destination": "/photography.html"
      },
      {
        "source": "/about-me",
        "destination": "/about.html"
      },
      {
        "source": "/minecraft",
        "destination": "/minecraftMusic.html"
      },
      {
        "source": "/games",
        "destination": "/games/index.html"
      },
      {
        "source": "/games/adjectives",
        "destination": "/games/adjective.html"
      },
      {
        "source": "/games/connect4",
        "destination": "/games/connect4.html"
      },
      {
        "source": "/games/gender",
        "destination": "/games/gender.html"
      },
      {
        "source": "/games/hangman",
        "destination": "/games/hangman.html"
      },
      {
        "source": "/games/terms",
        "destination": "/games/terms.html"
      },
      {
        "source": "/games/tictactoe",
        "destination": "/games/tictactoe.html"
      },
      {
        "source": "/games/2048",
        "destination": "/games/2048.html"
      }
    ]
  }
}
