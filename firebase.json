{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "indexes.json"
  },
  "hosting": {
    "site": "trinkey",
    "public": "public",
    "ignore": [
      "firebase.json",
      "deploy.bat",
      "**/.*",
      "**/node_modules/**",
      "public/template.html",
      ".gitignore"
    ],
    "rewrites": [
      {
        "source": "/changelog",
        "destination": "/patch.html"
      },
      {
        "source": "/photography",
        "destination": "/photography.html"
      },
      {
        "source": "/about-me",
        "destination": "/about.html"
      },
      {
        "source": "/minecraft",
        "destination": "/minecraftMusic.html"
      },
      {
        "source": "/games",
        "destination": "/games/index.html"
      },
      {
        "source": "/games/adjectives",
        "destination": "/games/adjective.html"
      },
      {
        "source": "/games/connect4",
        "destination": "/games/connect4.html"
      },
      {
        "source": "/games/hangman",
        "destination": "/games/hangman.html"
      },
      {
        "source": "/games/terms",
        "destination": "/games/terms.html"
      },
      {
        "source": "/games/tictactoe",
        "destination": "/games/tictactoe.html"
      },
      {
        "source": "/games/2048",
        "destination": "/games/2048.html"
      },
      {
        "source": "/games/uno",
        "destination": "/games/uno.html"
      },
      {
        "source": "/games/platformer",
        "destination": "/games/platformer.html"
      },
      {
        "source": "/other/",
        "destination": "/other/index.html"
      },
      {
        "source": "/other/gender",
        "destination": "/other/gender.html"
      },
      {
        "source": "/other/periodic",
        "destination": "/other/periodic.html"
      },
      {
        "source": "/other/pronoun",
        "destination": "/other/pronoun.html"
      },
      {
        "source": "/other/randomizer",
        "destination": "/other/sentenceRandomizer.html"
      }
    ]
  }
}
